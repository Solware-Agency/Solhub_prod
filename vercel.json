{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "src/index.js": {
      "maxDuration": 30
    },
    "api/download-pdf.js": {
      "maxDuration": 30
    }
  }
}